{
  "name": "angular-admin",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --browsers ChromeHeadless",
    "e2e": "ng e2e",
    "lint:css": "stylelint --color --fix \"**/*.scss\"",
    "lint:ts": "eslint --color --fix --ext .ts src",
    "lint:watch": "nodemon -e scss,ts -x npm run start-lint",
    "mock:test": "ng test --browsers ChromeHeadless --configuration=mock",
    "mock:start": "ng serve --configuration=mock",
    "serve": "node src/api/index.js",
    "start-lint": "npm run lint:css && npm run lint:ts",
    "start-mock": "npm-run-all --parallel lint:watch serve mock:start mock:test",
    "start-dev": "npm-run-all --parallel lint:watch test start"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.9",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/fire": "^6.0.0",
    "@angular/forms": "~9.1.9",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "firebase": "^7.14.6",
    "google-libphonenumber": "^3.2.8",
    "ngx-bootstrap": "^5.2.0",
    "ngx-intl-tel-input": "^2.3.4",
    "ngx-intl-tel-input-angular7": "0.0.3",
    "ngx-pagination": "^5.0.0",
    "ngx-toastr": "^10.2.0",
    "rxjs": "~6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1001.1",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@angular/language-service": "~9.1.9",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "codelyzer": "^5.1.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-angular": "^1.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-angular": "^4.0.1",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server-auth": "^1.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^20.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.8.3"
  }
}
